prefix=/opt/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.8.0
Libs: -L${libdir} -lfontconfig
Libs.private:  -lexpat -L/opt/local/lib -lfreetype -lz -lbz2 -liconv
Cflags: -I${includedir}
