prefix="/opt/tinyos-2.1.1-tools";
exec_prefix="${prefix}";
exec "/opt/nesc/bin/nescc-ncg" "-nescc=$exec_prefix/bin/ncc" "$@"
err=$?
echo "Couldn't execute /opt/nesc/bin/nescc-ncg" >&2
exit $err
